{
  "name": "foso-cdn",
  "version": "0.0.4",
  "description": "A webservice that dishes out files from npm packages.",
  "main": "index.js",
  "scripts": {
    "start": "node app",
    "test": "mocha test/**/*.js --reporter dot",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m \"update book\" && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am \"update book\" && git push git@github.com:fosojs/cdn gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fosojs/cdn.git"
  },
  "keywords": [
    "package",
    "manager",
    "cdn",
    "npm"
  ],
  "bin": {
    "fcdn": "./bin/fcdn.js"
  },
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fosojs/cdn/issues"
  },
  "homepage": "https://github.com/fosojs/cdn#readme",
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "dependencies": {
    "async": "^1.5.0",
    "boom": "^3.0.0",
    "catbox": "^7.0.0",
    "catbox-redis": "^1.0.6",
    "chalk": "^1.1.1",
    "clean-css": "^3.4.8",
    "commander": "^2.9.0",
    "convict": "^1.0.2",
    "css-url-rewrite": "^0.9.0",
    "debug": "^2.2.0",
    "duration-js": "^3.3.4",
    "findit": "^2.0.0",
    "handlebars": "^4.0.5",
    "hapi": "^11.1.1",
    "mongoose": "^4.3.4",
    "normalize-path": "^2.0.1",
    "npm-registry-client": "^7.0.9",
    "ramda": "^0.18.0",
    "semver": "^5.1.0",
    "tar-fs": "^1.8.1",
    "uglify-js": "^2.6.1",
    "update-notifier": "^0.6.0",
    "yaml-or-json": "^0.1.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "gitbook-cli": "^1.0.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4"
  }
}
